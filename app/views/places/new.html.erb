<div class="container">
  <div class="row">
<div class="enter-your-place-details col-10 offset-1" style="margin: auto">
  <h1>Enter Your Place Details</h1>
  <%= simple_form_for @place do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <br />
    <%= f.submit 'Create', class: 'btn btn-primary' %>
  <% end %>
</div>
</div>
</div>
